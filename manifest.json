{
  "name": "Samplr",
  "version": "0.1",
  "description": "Download music samples",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://sounds.com/*", "http://sounds.com/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
